{
    "extends": ["airbnb-base", "plugin:node/recommended"],
    "plugins": [
        "import",
        "node"
    ],
    "env": {
        "node": true
    },
    "rules": {
        "indent": ["error", 4],
        "no-console": "off",
        "node/exports-style": ["error", "module.exports"],
        "strict": "off",
        "no-process-exit": "off"
    }
}
